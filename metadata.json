{
  "name": "negz-lsyncd",
  "version": "1.2.2",
  "author": "Nic Cope",
  "license": "Apache-2.0",
  "summary": "Yet another lsyncd module",
  "source": "https://github.com/negz/puppet-lsyncd",
  "project_page": "https://forge.puppetlabs.com/negz/lsyncd",
  "issues_url": "https://github.com/negz/puppet-lsyncd/issues",
  "tags": ["lsyncd", "rsync"],
  "operatingsystem_support": [
    {
    "operatingsystem": "Ubuntu",
    "operatingsystemrelease": ["14.04","15.10"]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease":[
        "6"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "duritong/sysctl",
      "version_requirement": "0.0.11"
    },
    {
      "name": "puppet/systemd",
      "version": ">= 3.0.0"
    }
  ],
  "data_provider": "hiera"
}
